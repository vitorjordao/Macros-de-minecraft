log(&d» &6Mortasius pots em massa V1.2 &a[ON]);

#bau=1;
 

prompt(#qtd,$$?,Quantidade de baus unicos de pot que deseja.(maximo de 162));
if(%#qtd%>162);
log(&d» &6Macro nao suporta tudo isso de baus);
stop();
elseif(%#qtd%<1);
log(&d» &6Quantidade anormal de baus!);
stop();
endif;
prompt(#tipo,$$?,Qual pot deseja fazer?(1=Regeneracao / 2=Fogo / 3=Velocidade / 4=Força / =5=Vida ));
if(%#tipo%>5);
log(&d» &6Nao existe esse tipo de pot);
stop();
elseif(%#tipo%<1);
log(&d» &6Nao existe esse tipo de pot);
stop();
endif;
prompt(#tipo2,$$?,Deseja aumentar o tempo (1) ou a potência (2)?);
if(%#tipo2%>2);
log(&d» &6Tipo de "buff" na POT invalido! opcoes existentes->1/2);
stop();
elseif(%#tipo2%<1);
log(&d» &6Tipo de "buff" na POT invalido! opcoes existentes->1/2);
stop();
endif;
prompt(#lag,$$?,Escolha o delay (Se o servidor estiver pouco lag -> 2  / Muito lag -> 4 / Lag das 2h -> 6 ));
if(%#lag%<2);
#lag = 2
log(&d» &6Lag esta muito baixo! Ele foi redefinido para 2);
elseif(%#lag%>7);
log(&d» &6Seu servidor esta lagado D+ e pode dar problema... ligue o macro quando estiver menos lagado!);
stop;
endif
prompt(#naovip,$$?,Se for vip coloque 0 | Se nao for vip coloque 1);
if(%#naovip%=1);
#naovip = 5
elseif(%#naovip%<0);
log(&d» &6erro para saber se e ou nao vip);
stop;
elseif(%#naovip%>1);
log(&d» &6erro para saber se e ou nao vip);
stop;
endif;

#lag2 = %#lag% + %#naovip%
#lag3 = 0
#lag4 = 0
#lag5 = 0

if(%#lag%<4);
#lag3 = 303
#lag4 = 501
#lag5 = 35
elseif(%#lag%>=4)
#lag3=#lag*100
#lag3=#lag3+303
#lag4=#lag*500
#lag4=#lag4+501
#lag5=#lag*2
#lag5=#lag5+35
endif;

 
do(%#qtd%);


#baupot=1;

do;
wait(1000ms);
